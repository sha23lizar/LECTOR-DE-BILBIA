{
    "manifest_version":2,
    "name":"lector Biblico",
    "version":"1.1",
    "icons":{
        "128":"icon128.png",
        "32":"icon32.png",
        "16":"icon16.png"
    },
    "browser_action":{
        "default_icon":"icon16.png",
        "default_popup":"./popup/popup.html"
    }
    ,
    "permissions":[
        "storage",
        "notifications",
        "contextMenus",
        "tabs",
	"<all_urls>"
    ],

    "background":{
        "scripts":["eventPage.js"],
        "persistent":false
    },
    "options_page":"options/optionsPage.html"



}